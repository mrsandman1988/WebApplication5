@model List<Phone>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    @foreach(var item in Model)
    {
        <div style="display:inline-block; border:1px solid grey; margin-right:15px; padding:15px;">
            <img src="/images/@item.FileName" style="width:150px;" />
            <h1>@item.Name</h1>
            <h2>@item.Price</h2>
        </div>
       
    }
</body>
</html>
